    "def validation_node(state):\n",
    "    \"\"\"Validate generated output, stop after 3 reruns.\"\"\"\n",
    "    output = state[\"messages\"][-1]  # last generated message\n",
    "\n",
    "    # Retrieve current rerun count, default to 0 if not set\n",
    "    rerun_count = state.get(\"rerun_count\", 0)\n",
    "\n",
    "    # Check if weâ€™ve already rerun 3 times\n",
    "    if rerun_count >= 3:\n",
    "        print(\"Reached maximum reruns (3). Final output:\")\n",
    "        print(output)\n",
    "        return {\"messages\": [\"out\"]}   # go to end even if it fails\n",
    "\n",
    "    # Validation rules\n",
    "    if len(output.strip()) < 5 or \"I don't know\" in output:\n",
    "        # Increment and store count before requesting a rerun\n",
    "        return {\n",
    "            \"messages\": [\"rerun\"],\n",
    "            \"rerun_count\": rerun_count + 1\n",
    "        }\n",
    "\n",
    "    # If validation passes, reset/ignore rerun counter\n",
    "    return {\"messages\": [\"out\"], \"rerun_count\": rerun_count}\n"
   ]
  },
